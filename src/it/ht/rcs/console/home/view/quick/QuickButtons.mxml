<?xml version="1.0" encoding="utf-8"?>
<s:HGroup xmlns:fx="http://ns.adobe.com/mxml/2009"
          xmlns:s="library://ns.adobe.com/flex/spark"
          xmlns:mx="library://ns.adobe.com/flex/mx"
          gap="60">

  
  <fx:Script>
    <![CDATA[
      import mx.managers.PopUpManager;
      
      private function onQuickAgent():void
      {
        var popup:QuickAgentForm = PopUpManager.createPopUp(root, QuickAgentForm, true) as QuickAgentForm;
        PopUpManager.centerPopUp(popup);
      }
    ]]>
  </fx:Script>
  
  
  <s:Button width="150" label="Quick Agent Creation" click="onQuickAgent()"
            icon="@Embed('/img/NEW/home_agent.png')"/>
  
  <s:Button width="150" enabled="false" icon="@Embed('/img/NEW/home_empty.png')"/>
  
  <s:Button width="150" enabled="false" icon="@Embed('/img/NEW/home_empty.png')"/>

  
</s:HGroup>