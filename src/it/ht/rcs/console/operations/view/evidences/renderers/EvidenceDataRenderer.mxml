<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
                        xmlns:s="library://ns.adobe.com/flex/spark"
                        xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
      [Bindable]
			private var _data:*; 
			
			public function set data(value:*):void
			{ 
				_data=value;
			}
      public function get data():*
      {
        return _data;
      }
		]]>
	</fx:Script>
  <s:layout>
    <s:HorizontalLayout horizontalAlign="center" verticalAlign="middle" paddingBottom="8" paddingLeft="4" paddingRight="4" paddingTop="8"/>
  </s:layout>
</s:Group>