<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
         xmlns:s="library://ns.adobe.com/flex/spark"
         xmlns:mx="library://ns.adobe.com/flex/mx"
         height="100%" width="30">
  
  <s:Line xFrom="14" xTo="14" yFrom="1" yTo="{height-1}"> <!-- Remove '-1' for pure glitch awesomeness -->
    <s:stroke>
      <s:SolidColorStroke color="gray" weight="1"/>
    </s:stroke>
  </s:Line>
  
  <s:Line xFrom="15" xTo="15" yFrom="1" yTo="{height-1}"> <!-- Remove '-1' for pure glitch awesomeness -->
    <s:stroke>
      <s:SolidColorStroke color="#ffffff" weight="1"/>
    </s:stroke>
  </s:Line>
  
</s:Group>